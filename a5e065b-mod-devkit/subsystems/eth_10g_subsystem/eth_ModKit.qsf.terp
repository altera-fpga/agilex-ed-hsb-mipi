@@ set entity_name ${param0}

set_location_assignment PIN_AK135 -to serial_i_rx_serial_data
set_location_assignment PIN_AK133 -to serial_i_rx_serial_data_n

set_location_assignment PIN_AL129 -to serial_o_tx_serial_data
set_location_assignment PIN_AL126 -to serial_o_tx_serial_data_n

set_location_assignment PIN_AY120 -to refclk_10g

set_instance_assignment -name IO_STANDARD "CURRENT MODE LOGIC (CML)"    -to refclk_10g                -entity ${entity_name}
set_instance_assignment -name IO_STANDARD "HIGH SPEED DIFFERENTIAL I/O" -to serial_i_rx_serial_data   -entity ${entity_name}
set_instance_assignment -name IO_STANDARD "HIGH SPEED DIFFERENTIAL I/O" -to serial_i_rx_serial_data_n -entity ${entity_name}
set_instance_assignment -name IO_STANDARD "HIGH SPEED DIFFERENTIAL I/O" -to serial_o_tx_serial_data   -entity ${entity_name}
set_instance_assignment -name IO_STANDARD "HIGH SPEED DIFFERENTIAL I/O" -to serial_o_tx_serial_data_n -entity ${entity_name}
